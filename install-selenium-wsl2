wget https://dl.google.com/linux/direct/google-chrome-stable_current_amd64.deb

sudo apt install -y ./google-chrome-stable_current_amd64.deb

https://chromedriver.storage.googleapis.com/130.0.6723.91/chromedriver_linux64.zip
google-chrome --version

wget https://storage.googleapis.com/chrome-for-testing-public/130.0.6723.91/linux64/chromedriver-linux64.zip

unzip chromedriver-linux64.zip
ls -lia

sudo mv chromedriver-linux64 /usr/local/bin/chromedriver

ls /usr/local/bin/chromedriver
